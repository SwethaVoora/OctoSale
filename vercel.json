{
  "version": 2,
  "builds": [
    {
      "src": "src/api/trpc/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/trpc/(.*)",
      "dest": "/api/trpc/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
