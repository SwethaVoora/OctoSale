{
  "version": 2,
  "builds": [
    {
      "src": "src/trpc/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "next.config.mjs",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/trpc/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
